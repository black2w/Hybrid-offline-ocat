<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>


	<typeAliases>
		<!-- 基础类型 -->
		<typeAlias alias="Integer" type="java.lang.Integer" />
		<typeAlias alias="Long" type="java.lang.Long" />
		<typeAlias alias="HashMap" type="java.util.HashMap" />
		<typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap" />
		<typeAlias alias="ArrayList" type="java.util.ArrayList" />
		<typeAlias alias="LinkedList" type="java.util.LinkedList" />
		<typeAlias alias="HashSet" type="java.util.HashSet" />
		<!-- 资源与补丁 -->
		<typeAlias alias="Resource" type="com.lhc.ocat.mobileplatform.domain.dos.ResourceDO" />
		<typeAlias alias="Patch" type="com.lhc.ocat.mobileplatform.domain.dos.PatchDO" />
		<!--系统管理-->
		<typeAlias alias="User" type="com.lhc.ocat.mobileplatform.domain.dos.UserDO"/>
		<typeAlias alias="Role" type="com.lhc.ocat.mobileplatform.domain.dos.RoleDO"/>
		<typeAlias alias="Permission" type="com.lhc.ocat.mobileplatform.domain.dos.PermissionDO"/>
		<typeAlias alias="Menu" type="com.lhc.ocat.mobileplatform.domain.dos.MenuDO"/>
	</typeAliases>
</configuration>